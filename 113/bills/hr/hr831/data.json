  "introduced_at": "2013-02-26", 
  "number": "831", 
  "official_title": "To phase out special wage certificates under the Fair Labor Standards Act of 1938 under which individuals with disabilities may be employed at subminimum wage rates.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Fair Wages for Workers with Disabilities Act of 2013", 
  "sponsor": {
    "district": "3", 
