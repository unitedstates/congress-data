  "introduced_at": "2013-06-27", 
  "number": "2565", 
  "official_title": "To provide for the termination of employment of employees of the Internal Revenue Service who take certain official actions for political purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "STOP IRS Act", 
