  "introduced_at": "2003-04-02", 
  "number": "1563", 
  "official_title": "To require engine coolant and antifreeze to contain a bittering agent so as to render it unpalatable.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
