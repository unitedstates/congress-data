  "introduced_at": "2003-02-11", 
  "number": "693", 
  "official_title": "To amend the Internal Revenue Code of 1986 to exclude from gross income certain death gratuity payments to members of the uniformed services.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Military Death Gratuity Improvement Act of 2003", 
