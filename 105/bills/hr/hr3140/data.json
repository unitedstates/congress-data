  "introduced_at": "1998-02-03", 
  "number": "3140", 
  "official_title": "To amend title 18, United States Code, to provide that certain muzzle loading firearms are to be treated as antique firearms for purposes of the Federal firearms laws.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "3", 
    "name": "Boswell, Leonard L.", 
