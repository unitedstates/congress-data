  "introduced_at": "1995-05-11", 
  "number": "1610", 
  "official_title": "To amend the Internal Revenue Code of 1986 to require employer-provided group health plans to credit coverage under a prior group health plan against any preexisting condition limitation.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
