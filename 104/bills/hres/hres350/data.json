    {
      "acted_at": "1996-01-31T17:04:00-05:00", 
      "references": [], 
      "text": "Consideration initiated as a question of the privileges of the House.", 
      "type": "action"
    }, 
    {
      "acted_at": "1996-01-31T17:04:00-05:00", 
      "references": [], 
      "text": "Members of the House were recognized to speak on whether the resolution consituted a question of the privileges of the House.", 
      "type": "action"
    }, 
    {
      "acted_at": "1996-01-31T17:31:00-05:00", 
      "references": [], 
      "text": "The Chair ruled that the resolution did not constitute a question of the privileges of the House and, in addition, no question of personal privilege under rule IX of the Rules of the House was addressed by this resolution.", 
      "type": "action"
    }, 
    {
      "acted_at": "1996-01-31T17:32:00-05:00", 
      "references": [], 
      "text": "Mrs. Mink moved to appeal the ruling of the Chair.", 
      "type": "action"
    }, 
    {
      "acted_at": "1996-01-31T17:35:00-05:00", 
      "references": [], 
      "text": "Ms. Pryce moved to table the motion to appeal the ruling of the Chair", 
      "type": "action"
    }, 
    {
      "acted_at": "1996-01-31T17:36:00-05:00", 
      "references": [], 
      "text": "Motion to appeal the ruling of the Chair tabled.", 
      "type": "action"
    }, 
    {
      "acted_at": "1996-01-31T17:36:00-05:00", 
      "references": [], 
      "text": "On motion to table the motion to appeal the ruling of the Chair Agreed to by voice vote.", 
      "type": "action"
    }
