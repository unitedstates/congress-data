  "introduced_at": "2006-03-14", 
  "number": "4949", 
  "official_title": "To amend title 10, United States Code, to prohibit increases in fees for military health care.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Military Retirees' Health Care Protection Act", 
  "sponsor": {
    "district": "17", 
    "name": "Edwards, Chet", 
