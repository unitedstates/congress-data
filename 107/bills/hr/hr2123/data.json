  "introduced_at": "2001-06-12", 
  "number": "2123", 
  "official_title": "To amend title 38, United States Code, to increase the rate of payment for funeral and burial expenses and plot allowance for certain veterans.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
