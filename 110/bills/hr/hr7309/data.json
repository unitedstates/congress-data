  "introduced_at": "2008-12-09", 
  "number": "7309", 
  "official_title": "To amend the Internal Revenue Code of 1986 to suspend employment and income taxes for the first two months of 2009, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
