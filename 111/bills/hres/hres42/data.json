  "introduced_at": "2009-01-09", 
  "number": "42", 
  "official_title": "Calling on the President and the Secretary of State to withhold United States funding for and participation in the Durban Review Conference and its preparatory activities, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "18", 
    "name": "Ros-Lehtinen, Ileana", 
